<!doctype html>
<html>
  <head>
    <meta charset='utf-8'>

    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content='IE=edge,chrome=1' http-equiv='X-UA-Compatible'>

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || 'Did It' %></title>

    <%= stylesheet_link_tag 'normalize', 'application' %>

    <!-- React -->
    <%= javascript_include_tag 'https://fb.me/react-0.14.6.min.js' %>
    <%= javascript_include_tag 'https://fb.me/react-dom-0.14.6.min.js' %>
  </head>

  <body class='<%= page_classes %>'>
    <%= yield %>
    <!-- Components -->
    <%= javascript_include_tag 'build/script.js' %> 
  </body>
</html>
